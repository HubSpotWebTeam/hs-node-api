{
  "name": "hubspot-api",
  "version": "1.3.0",
  "description": "HubSpot API Wrapper",
  "keywords": [
    "hubspot",
    "hubspot-api"
  ],
  "repository": "github:HubSpotWebTeam/hs-node-api",
  "main": "dist/main.js",
  "scripts": {
    "api-tests": "DEBUG=utilities babel-node --presets node6 test/",
    "build": "rimraf dist/ docs/ && DEBUG=rollup:compile node tools/build.js && npm run compile-docs",
