{
  "extends": "airbnb",
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "no-bitwise": 0,
    "comma-dangle": 0,
    "no-param-reassign": 1,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "arrow-parens": 0,
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "no-plusplus": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/require-extension": "off",
    "react/prefer-stateless-function": "off",
    "react/forbid-prop-types": "off",
    "complexity": "off",
    "comma-spacing": "off",
    "array-bracket-spacing": "off",
    "max-len": 0,
    "no-unused-vars": "warn"
  },
  "globals": {
    "$": true,
    "document": true,
    "window": true,
    "navigator": true,
    "localStorage": true,
    "jQuery": true,
    "Request": true,
    "Headers": true,
    "fetch": true,
    "describe": true,
    "xdescribe": true,
    "xit": true,
    "it": true
  }
}
